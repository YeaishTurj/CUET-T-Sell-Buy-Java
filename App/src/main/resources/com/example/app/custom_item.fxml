<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox fx:id="customItem" prefHeight="79.0" prefWidth="593.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.customDesign.CustomItemController">
   <children>
      <ImageView fx:id="itemImage" fitHeight="82.0" fitWidth="81.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/product_2.jpg" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" prefHeight="84.0" prefWidth="460.0">
         <children>
            <Text fx:id="itemTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" wrappingWidth="361.21630859375" />
            <Text fx:id="itemPrice" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" wrappingWidth="365.21630859375" />
            <Text fx:id="itemQuantity" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" wrappingWidth="380.21630859375" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="more" mnemonicParsing="false" onMouseClicked="#seeDetails" prefHeight="45.0" prefWidth="47.0" text="More" />
         </children>
      </VBox>
   </children>
</HBox>
