<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="100.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.customDesign.CustomItemController">
   <HBox prefHeight="96.0" prefWidth="510.0" style="-fx-background-color: #CBD6e2;">
      <HBox.margin>
         <Insets bottom="10.0" left="10.0" right="20.0" top="10.0" />
      </HBox.margin>
      <ImageView fx:id="itemImage1" fitHeight="113.0" fitWidth="176.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@../../../assets/product_2.jpg" />
      </ImageView>
      <VBox alignment="CENTER" prefHeight="89.0" prefWidth="264.0">
         <Text fx:id="itemTitle1" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" />
         <Text fx:id="itemPrice1" strokeType="OUTSIDE" strokeWidth="0.0" text=" shshhs" textAlignment="CENTER" />
         <Text fx:id="itemQuantity1" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" />
      </VBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <Button fx:id="more1" mnemonicParsing="false" onAction="#seeDetails1" prefHeight="34.0" prefWidth="76.0" text="More" />
      </VBox>
   </HBox>
   <HBox prefHeight="96.0" prefWidth="510.0" style="-fx-background-color: #cbd6e2;">
      <HBox.margin>
         <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
      </HBox.margin>
      <ImageView fx:id="itemImage2" fitHeight="113.0" fitWidth="176.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@../../../assets/product_2.jpg" />
      </ImageView>
      <VBox alignment="CENTER" prefHeight="109.0" prefWidth="303.0" style="-fx-background-color: #cbd6e2;">
         <Text fx:id="itemTitle2" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" />
         <Text fx:id="itemPrice2" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" />
         <Text fx:id="itemQuantity2" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" />
      </VBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <Button fx:id="more2" mnemonicParsing="false" onAction="#seeDetails2" prefHeight="37.0" prefWidth="76.0" text="More" />
      </VBox>
   </HBox>
</HBox>
